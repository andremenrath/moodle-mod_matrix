{
  "name": "element-io/matrix-moodle-plugin",
  "description": "Moodle plugin for Matrix integration",
  "license": "GPL-3.0-only",
  "authors": [
    {
      "name": "New Vector Ltd. (trading as Element)",
      "email": "support@element.io"
    }
  ],
  "require": {
    "php": "^7.2 || ^8.0",
    "ext-json": "*",
    "php-curl-class/php-curl-class": "^9.2.0"
  },
  "require-dev": {
    "ergebnis/composer-normalize": "^2.8.0",
    "ergebnis/test-util": "^1.5.0",
    "friendsofphp/php-cs-fixer": "^3.0.2",
    "moodle/moodle": "3.9.5",
    "phpunit/phpunit": "^8.5.19",
    "vimeo/psalm": "^4.9.3"
  },
  "config": {
    "platform": {
      "php": "7.2.33"
    },
    "sort-packages": true
  },
  "autoload": {
    "psr-4": {
      "mod_matrix\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "mod_matrix\\Test\\": "test/"
    }
  }
}
