{
  "name": "element-io/matrix-moodle-plugin",
  "description": "Moodle plugin for Matrix integration",
  "license": "GPL-3.0-only",
  "authors": [
    {
      "name": "New Vector Ltd. (trading as Element)",
      "email": "support@element.io"
    }
  ],
  "require": {
    "php": "^7.3 || ^8.0",
    "ext-json": "*",
    "ergebnis/clock": "^2.3.0",
    "php-curl-class/php-curl-class": "^9.5.0"
  },
  "require-dev": {
    "ergebnis/composer-normalize": "^2.18.0",
    "ergebnis/data-provider": "^1.0.0",
    "ergebnis/php-cs-fixer-config": "~3.4.0",
    "fakerphp/faker": "^1.17.0",
    "moodle/moodle": "3.9.5",
    "phpunit/phpunit": "^9.5.10",
    "psalm/plugin-phpunit": "~0.16.1",
    "symfony/var-dumper": "^5.4.1",
    "vimeo/psalm": "^4.14.0"
  },
  "autoload": {
    "psr-4": {
      "mod_matrix\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "mod_matrix\\Test\\": "test/"
    }
  },
  "config": {
    "platform": {
      "php": "7.3.30"
    },
    "sort-packages": true
  },
  "extra": {
    "allow-plugins": {
      "ergebnis/composer-normalize": true
    }
  }
}
